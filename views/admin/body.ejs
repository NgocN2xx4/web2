<div class="main-panel">
    <div class="content-wrapper">
        <!-- Đảo ngược thứ tự: Bắt đầu bằng "Trạng Thái Đơn Hàng" -->
        <div class="row">
            <div class="col-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Trạng Thái Đơn Hàng</h4>
                        <div class="table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>
                                            <div class="form-check form-check-muted m-0">
                                                <label class="form-check-label">
                                                    <input type="checkbox" class="form-check-input">
                                                </label>
                                            </div>
                                        </th>
                                        <th>Tên Khách Hàng</th>
                                        <th>Mã Đơn Hàng</th>
                                        <th>Chi Phí Sản Phẩm</th>
                                        <th>Hình Thức Thanh Toán</th>
                                        <th>Hình Thức Giao Hàng</th>
                                        <th>Ngày Bắt Đầu</th>
                                        <th>Trạng Thái Thanh Toán</th>
                                    </tr>
                                </thead>
                                <tbody>
                                <% for(var item of order_items) { %>
                                    <tr>
                                        <td>
                                            <div class="form-check form-check-muted m-0">
                                                <label class="form-check-label">
                                                    <input type="checkbox" class="form-check-input">
                                                </label>
                                            </div>
                                        </td>
                                        <td>
                                            <% if(item.user_id) { %>
                                                <img src="<%= item.user_id.image %>" alt="image" />
                                                <span class="pl-2">
                                                    <%= item.user_id.name %>
                                                </span>
                                            <% } else { %>
                                                <span class="pl-2 text-danger">Khách Hàng Không Xác Định</span>
                                            <% } %>                 
                                        </td>
                                        <td>02312</td>
                                        <td><%= item.total_money.toLocaleString('vi-VN') %> ₫</td>
                                        <td><%= item.payment_status %></td>
                                        <td><%= item.delivery_status %></td>
                                        <td><%= formatDate(item.created_at) %></td>
                                        <td>
                                            <% if(item.delivery_status == 'processing') { %>
                                                <div class="badge badge-outline-success">Đã Xác Nhận</div>
                                            <% } else if(item.delivery_status == 'Delivered') { %>
                                                <div class="badge badge-outline-primary">Đã Giao</div>
                                            <% } else { %>
                                                <div class="badge badge-outline-danger">Đã Hủy</div>
                                            <% } %>
                                        </td>
                                    </tr>
                                <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Thay đổi thứ tự và sắp xếp các thẻ (card) theo hàng ngang -->
        <div class="row">
            <div class="col-sm-4 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h5>Doanh Thu</h5>
                        <div class="row">
                            <div class="col-8 col-sm-12 col-xl-8 my-auto">
                                <div class="d-flex d-sm-block d-md-flex align-items-center">
                                    <h2 class="mb-0"><%= (total_revenue * 23000).toLocaleString('vi-VN') %> ₫</h2>
                                    <p class="text-success ml-2 mb-0 font-weight-medium">+3.5%</p>
                                </div>
                                <h6 class="text-muted font-weight-normal">11.38% So với tháng trước</h6>
                            </div>
                            <div class="col-4 col-sm-12 col-xl-4 text-center text-xl-right">
                                <i class="icon-lg mdi mdi-codepen text-primary ml-auto"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-4 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h5>Doanh Số</h5>
                        <div class="row">
                            <div class="col-8 col-sm-12 col-xl-8 my-auto">
                                <div class="d-flex d-sm-block d-md-flex align-items-center">
