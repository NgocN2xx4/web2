<div class="main-panel">
    <div class="content-wrapper">
        <!-- Trạng Thái Đơn Hàng -->
        <div class="row">
            <div class="col-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Trạng Thái Đơn Hàng</h4>
                        <div class="table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>
                                            <div class="form-check form-check-muted m-0">
                                                <label class="form-check-label">
                                                    <input type="checkbox" class="form-check-input">
                                                </label>
                                            </div>
                                        </th>
                                        <th>Tên Khách Hàng</th>
                                        <th>Mã Đơn Hàng</th>
                                        <th>Chi Phí Sản Phẩm</th>
                                        <th>Hình Thức Thanh Toán</th>
                                        <th>Hình Thức Giao Hàng</th>
                                        <th>Ngày Bắt Đầu</th>
                                        <th>Trạng Thái Thanh Toán</th>
                                    </tr>
                                </thead>
                                <tbody>
                                <% for(var item of order_items) { %>
                                    <tr>
                                        <td>
                                            <div class="form-check form-check-muted m-0">
                                                <label class="form-check-label">
                                                    <input type="checkbox" class="form-check-input">
                                                </label>
                                            </div>
                                        </td>
                                        <td>
                                            <% if(item.user_id) { %>
                                                <img src="<%= item.user_id.image %>" alt="image" />
                                                <span class="pl-2">
                                                    <%= item.user_id.name %>
                                                </span>
                                            <% } else { %>
                                                <span class="pl-2 text-danger">Khách Hàng Không Xác Định</span>
                                            <% } %>                 
                                        </td>
                                        <td>02312</td>
                                        <td><%= item.total_money.toLocaleString('vi-VN') %> ₫</td>
                                        <td><%= item.payment_status %></td>
                                        <td><%= item.delivery_status %></td>
                                        <td><%= formatDate(item.created_at) %></td>
                                        <td>
                                            <% if(item.delivery_status == 'processing') { %>
                                                <div class="badge badge-outline-success">Đã Xác Nhận</div>
                                            <% } else if(item.delivery_status == 'Delivered') { %>
                                                <div class="badge badge-outline-primary">Đã Giao</div>
                                            <% } else { %>
                                                <div class="badge badge-outline-danger">Đã Hủy</div>
                                            <% } %>
                                        </td>
                                    </tr>
                                <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Doanh Thu, Doanh Số, Giá Bán -->
        <div class="row">
            <div class="col-sm-4 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h5>Doanh Thu</h5>
                        <div class="row">
                            <div class="col-8 col-sm-12 col-xl-8 my-auto">
                                <div class="d-flex d-sm-block d-md-flex align-items-center">
                                    <h2 class="mb-0"><%= (total_revenue * 23000).toLocaleString('vi-VN') %> ₫</h2>
                                    <p class="text-success ml-2 mb-0 font-weight-medium">+3.5%</p>
                                </div>
                                <h6 class="text-muted font-weight-normal">11.38% So với tháng trước</h6>
                            </div>
                            <div class="col-4 col-sm-12 col-xl-4 text-center text-xl-right">
                                <i class="icon-lg mdi mdi-codepen text-primary ml-auto"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-4 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h5>Doanh Số</h5>
                        <div class="row">
                            <div class="col-8 col-sm-12 col-xl-8 my-auto">
                                <div class="d-flex d-sm-block d-md-flex align-items-center">
                                    <h2 class="mb-0"><%= (discount * 23000).toLocaleString('vi-VN') %> ₫</h2>
                                    <p class="text-success ml-2 mb-0 font-weight-medium">+8.3%</p>
                                </div>
                                <h6 class="text-muted font-weight-normal">9.61% So với tháng trước</h6>
                            </div>
                            <div class="col-4 col-sm-12 col-xl-4 text-center text-xl-right">
                                <i class="icon-lg mdi mdi-wallet-travel text-danger ml-auto"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-4 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h5>Giá Bán</h5>
                        <div class="row">
                            <div class="col-8 col-sm-12 col-xl-8 my-auto">
                                <div class="d-flex d-sm-block d-md-flex align-items-center">
                                    <h2 class="mb-0"><%= (price * 23000).toLocaleString('vi-VN') %> ₫</h2>
                                    <p class="text-danger ml-2 mb-0 font-weight-medium">-2.1%</p>
                                </div>
                                <h6 class="text-muted font-weight-normal">2.27% So với tháng trước</h6>
                            </div>
                            <div class="col-4 col-sm-12 col-xl-4 text-center text-xl-right">
                                <i class="icon-lg mdi mdi-monitor text-success ml-auto"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Tổng Sản Phẩm, Tổng Đơn Hàng, Tổng Khách Hàng, Tổng Phản Hồi -->
        <div class="row">
            <div class="col-xl-3 col-sm-6 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-9">
                                <div class="d-flex align-items-center align-self-start">
                                    <h3 class="mb-0"><%= products.length %></h3>
                                    <p class="text-success ml-2 mb-0 font-weight-medium">+3.5%</p>
                                </div>
                            </div>
                            <div class="col-3">
                                <div class="icon icon-box-success">
                                    <span class="mdi mdi-arrow-top-right icon-item"></span>
                                </div>
                            </div>
                        </div>
                        <h6 class="text-muted font-weight-normal">Tổng Sản Phẩm</h6>
                    </div>
                </div>
            </div>
            <div class="col-xl-3 col-sm-6 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-9">
                                <div class="d-flex align-items-center align-self-start">
                                    <h3 class="mb-0"><%= orders.length %></h3>
                                    <p class="text-success ml-2 mb-0 font-weight-medium">+11%</p>
                                </div>
                            </div>
                            <div class="col-3">
                                <div class="icon icon-box-success">
                                    <span class="mdi mdi-arrow-top-right icon-item"></span>
                                </div>
                            </div>
                        </div>
                        <h6 class="text-muted font-weight-normal">Tổng Đơn Hàng</h6>
                    </div>
                </div>
            </div>
            <div class="col-xl-3 col-sm-6 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-9">
                                <div class="d-flex align-items-center align-self-start">
                                    <h3 class="mb-0"><%= users.length %></h3>
                                    <p class="text-danger ml-2 mb-0 font-weight-medium">-2.4%</p>
                                </div>
                            </div>
                            <div class="col-3">
                                <div class="icon icon-box-danger">
                                    <span class="mdi mdi-arrow-bottom-left icon-item"></span>
                                </div>
                            </div>
                        </div>
                        <h6 class="text-muted font-weight-normal">Tổng Khách Hàng</h6>
                    </div>
                </div>
            </div>
            <div class="col-xl-3 col-sm-6 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-9">
                                <div class="d-flex align-items-center align-self-start">
                                    <h3 class="mb-0">0</h3>
                                    <p class="text-success ml-2 mb-0 font-weight-medium">+3.5%</p>
                                </div>
                            </div>
                            <div class="col-3">
                                <div class="icon icon-box-success">
                                    <span class="mdi mdi-arrow-top-right icon-item"></span>
                                </div>
                            </div>
                        </div>
                        <h6 class="text-muted font-weight-normal">Tổng Phản Hồi</h6>
                    </div>
                </div>
            </div>
        </div>

        <!-- Tin Nhắn, Hình Ảnh Hồ Sơ, Danh Sách Công Việc -->
        <div class="row">
            <div class="col-md-6 col-xl-4 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Tin Nhắn</h4>
                        <div class="d-flex flex-row justify-content-between">
                            <p class="text-muted mb-1 small">Xem tất cả</p>
                        </div>
                        <div class="preview-list">
                            <div class="preview-item border-bottom">
                                <div class="preview-item-content d-flex flex-grow">
                                    <div class="flex-grow">
                                        <h6 class="preview-subject">Không có tin nhắn</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-xl-4 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Hình Ảnh Hồ Sơ</h4>
                        <div class="owl-carousel owl-theme full-width owl-carousel-dash portfolio-carousel" id="owl-carousel-basic">
                            <div class="item">
                                <img src="../../themes/admin/assets/images/dashboard/Rectangle.jpg" alt="">
                            </div>
                            <div class="item">
                                <img src="../../themes/admin/assets/images/dashboard/Img_5.jpg" alt="">
                            </div>
                            <div class="item">
                                <img src="../../themes/admin/assets/images/dashboard/img_6.jpg" alt="">
                            </div>
                        </div>
                        <div class="d-flex py-4">
                            <div class="preview-list w-100">
                                <div class="preview-item p-0">
                                    <div class="preview-thumbnail">
                                        <img src="../../themes/admin/assets/images/in4.jpg" class="rounded-circle" alt="">
                                    </div>
                                    <div class="preview-item-content d-flex flex-grow">
                                        <div class="flex-grow">
                                            <div class="d-flex d-md-block d-xl-flex justify-content-between">
                                                <h6 class="preview-subject">Ngọc Nguyễn</h6>
                                                <p class="text-muted text-small">4 giờ trước</p>
                                            </div>
                                            <p class="text-muted">Hoạt động bình thường.</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <p class="text-muted">Hoạt động bình thường.</p>
                        <div class="progress progress-md portfolio-progress">
                            <div class="progress-bar bg-success" role="progressbar" style="width: 50%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-12 col-xl-4 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Danh Sách Công Việc</h4>
                        <div class="add-items d-flex">
                            <input type="text" class="form-control todo-list-input" placeholder="Nhập công việc...">
                            <button class="add btn btn-primary todo-list-add-btn">Thêm</button>
                        </div>
                        <div class="list-wrapper">
                            <ul class="d-flex flex-column-reverse text-white todo-list todo-list-custom">
                                <li>
                                    <div class="form-check form-check-primary">
                                        <label class="form-check-label">
                                            <input class="checkbox" type="checkbox"> Tạo hóa đơn </label>
                                        </div>
                                        <i class="remove mdi mdi-close-box"></i>
                                    </li>
                                    <li>
                                        <div class="form-check form-check-primary">
                                            <label class="form-check-label">
                                                <input class="checkbox" type="checkbox"> Gặp gỡ Phạm Nhật Vượng </label>
                                            </div>
                                            <i class="remove mdi mdi-close-box"></i>
                                        </li>
                                        <li class="completed">
                                            <div class="form-check form-check-primary">
                                                <label class="form-check-label">
                                                    <input class="checkbox" type="checkbox" checked> Giao hàng đơn 0987654321 </label>
                                                </div>
                                                <i class="remove mdi mdi-close-box"></i>
                                            </li>
                                            <li>
                                                <div class="form-check form-check-primary">
                                                    <label class="form-check-label">
                                                        <input class="checkbox" type="checkbox"> Giao hàng đơn 0987654321 </label>
                                                    </div>
                                                    <i class="remove mdi mdi-close-box"></i>
                                                </li>
                                                <li>
                                                    <div class="form-check form-check-primary">
                                                        <label class="form-check-label">
                                                            <input class="checkbox" type="checkbox"> Giao hàng đơn 0397297919 </label>
                                                        </div>
                                                        <i class="remove mdi mdi-close-box"></i>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>

        <!-- Lượng Khách Truy Cập Trong Ngày (không có bản đồ) -->
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Lượng Khách Truy Cập Trong Ngày</h4>
                        <div class="table-responsive">
                            <table class="table">
                                <tbody>
                                    <tr>
                                        <td>07:00 - 09:00</td>
                                        <td class="text-right">150</td>
                                        <td class="text-right font-weight-medium">12.5%</td>
                                    </tr>
                                    <tr>
                                        <td>09:00 - 12:00</td>
                                        <td class="text-right">300</td>
                                        <td class="text-right font-weight-medium">25.0%</td>
                                    </tr>
                                    <tr>
                                        <td>12:00 - 15:00</td>
                                        <td class="text-right">400</td>
                                        <td class="text-right font-weight-medium">33.3%</td>
                                    </tr>
                                    <tr>
                                        <td>15:00 - 18:00</td>
                                        <td class="text-right">250</td>
                                        <td class="text-right font-weight-medium">20.8%</td>
                                    </tr>
                                    <tr>
                                        <td>18:00 - 24:00</td>
                                        <td class="text-right">100</td>
                                        <td class="text-right font-weight-medium">8.3%</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- content-wrapper ends -->
    <!-- partial:partials/_footer.html -->
    <footer class="footer">
        <div class="d-sm-flex justify-content-center justify-content-sm-between">
            <span class="text-muted d-block text-center text-sm-left d-sm-inline-block">© 2023 Bản quyền thuộc về NgocStore</span>
            <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center"> Phân phối bởi <a href="https://myporfolio-434bc.firebaseapp.com/" target="_blank">NgocStore</a></span>
        </div>
